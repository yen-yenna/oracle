ALTER TABLE FAKTURA DROP CONSTRAINT klient_fk;
ALTER TABLE FAKTURA DROP CONSTRAINT faktura_pk;
ALTER TABLE FAKTURA_DETALE DROP CONSTRAINT faktura_detale_pk;
ALTER TABLE ZWIERZAKI DROP CONSTRAINT zwierzaki_pk;
ALTER TABLE GATUNEK DROP CONSTRAINT gatunek_pk;
ALTER TABLE PLATNOSC DROP CONSTRAINT platnosc_pk;
ALTER TABLE klient DROP CONSTRAINT klient_pk;
ALTER TABLE pracownicy DROP CONSTRAINT pracownicy_pk;

DROP TABLE IF EXISTS FAKTURA;
DROP TABLE IF EXISTS FAKTURA_DETALE;
DROP TABLE IF EXISTS ZWIERZAKI;
DROP TABLE IF EXISTS PLATNOSC;
DROP TABLE IF EXISTS KLIENT;
DROP TABLE IF EXISTS PRACOWNICY;
DROP TABLE IF EXISTS GATUNEK;


DROP SEQUENCE  SEQ_faktura;
DROP SEQUENCE SEQ_klient1;
DROP SEQUENCE SEQ_pracownicy;
